{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^24"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=line",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint:eslint:ci": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "test:cucumber": "cucumber-js --loader ts-node/esm --import './features/step_definitions/**/*.ts'"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bun": "^1.3.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/integrations": "^14.0.0",
    "axios": "^1.13.2",
    "bun": "^1.3.3",
    "jose": "^6.1.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "qrcode": "^1.5.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vuetify": "^3.11.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.57.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.5",
    "@types/cucumber": "^7.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/eslint-plugin": "^1.5.0",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "^1.30.0",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-vue": "^10.6.1",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.30.0",
    "prettier": "3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-csp-guard": "^3.0.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  }
}

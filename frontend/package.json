{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=line",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint:eslint:ci": "eslint . --fix --no-watch",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "prepare": "cd .. && husky",
    "test:cucumber": "cucumber-js --loader ts-node/esm --import './features/step_definitions/**/*.ts'"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@playwright/test": "^1.56.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/cucumber": "^7.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.3.16",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "~1.10.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.10.0",
    "prettier": "3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}

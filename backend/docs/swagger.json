{
    "swagger": "2.0",
    "info": {
        "description": "This is the API for the RoomPlay service.",
        "title": "RoomPlay API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:7865",
    "basePath": "/",
    "paths": {
        "/api/v1/room": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Creates a new room in the system",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "rooms"
                ],
                "summary": "Create a new room",
                "parameters": [
                    {
                        "description": "Create Room",
                        "name": "room",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/room.CreateCommand"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/response.Success"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    }
                }
            }
        },
        "/api/v1/user": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Creates a new user in the system",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Logins a new user with data from OAuth",
                "parameters": [
                    {
                        "description": "Login user",
                        "name": "room",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/user.CreateCommand"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/response.Success"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/response.Error"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "entities.DeviceType": {
            "type": "string",
            "enum": [
                "MOBILE",
                "COMPUTER"
            ],
            "x-enum-varnames": [
                "MOBILE",
                "COMPUTER"
            ]
        },
        "response.Error": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string",
                    "x-nullable": true,
                    "example": "null"
                },
                "isSuccess": {
                    "type": "boolean",
                    "example": false
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "response.Success": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object"
                },
                "isSuccess": {
                    "type": "boolean"
                },
                "message": {
                    "type": "string",
                    "x-nullable": true,
                    "example": "null"
                }
            }
        },
        "room.CreateCommand": {
            "type": "object",
            "required": [
                "roomName",
                "roomPassword",
                "userId"
            ],
            "properties": {
                "roomName": {
                    "type": "string"
                },
                "roomPassword": {
                    "type": "string"
                },
                "userId": {
                    "type": "string"
                }
            }
        },
        "user.CreateCommand": {
            "type": "object",
            "required": [
                "device",
                "external_id",
                "name",
                "surname"
            ],
            "properties": {
                "device": {
                    "$ref": "#/definitions/user.DeviceDTO"
                },
                "external_id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "surname": {
                    "type": "string"
                }
            }
        },
        "user.DeviceDTO": {
            "type": "object",
            "required": [
                "device_type",
                "fingerprint",
                "friendly_name"
            ],
            "properties": {
                "device_type": {
                    "$ref": "#/definitions/entities.DeviceType"
                },
                "fingerprint": {
                    "type": "string"
                },
                "friendly_name": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "BearerAuth": {
            "description": "Type \"Bearer {token}\"",
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}